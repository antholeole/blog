---
import BaseHead from './BaseHead.astro';

const { title, description } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={title} description={description} />
	</head>

	<body>		
		<article>
			<h1>{title}</h1>
			<slot name="subheader" />
			<section>
	            <slot />
			</section>
        </article>
	</body>
</html>
